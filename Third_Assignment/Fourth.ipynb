   "source": [
    "def even_numbers(limit):\n",
    "    num = 0\n",
    "    while num <= limit:\n",
    "        yield num\n",
    "        num += 2\n",
    "\n",
    "# Example usage:\n",
    "my_generator = even_numbers(10)\n",
    "\n",
    "for num in my_generator:\n",
    "    print(num)\n"
   ]
